{
  "expo": {
    "name": "B2CApp",
    "slug": "kamathlaundryb2capp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A57A2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "kamathlaundry.b2c.app",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "844a1c8f-c405-436d-9198-d812be904bc3"
      }
    },
    "owner": "narasimhakamath",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Dealerplate.otf",
            "./src/assets/fonts/Instruction.otf",
            "./src/assets/fonts/InstructionBold.otf",
            "./src/assets/fonts/Huli.ttf",
            "./src/assets/fonts/LTSaeada.otf",
            "./src/assets/fonts/LTSaeadaBold.otf",
            "./src/assets/fonts/LTSaeadaExtraBold.otf",
            "./src/assets/fonts/LTSaeadaExtraLight.otf",
            "./src/assets/fonts/LTSaeadaHairline.otf",
            "./src/assets/fonts/LTSaeadaLight.otf",
            "./src/assets/fonts/LTSaeadaMedium.otf",
            "./src/assets/fonts/LTSaeadaRegular.otf",
            "./src/assets/fonts/LTSaeadaSemiBold.otf",
            "./src/assets/fonts/LTSaeadaThin.otf",
            "./src/assets/fonts/AlteHaasGroteskBold.ttf",
            "./src/assets/fonts/AlteHaasGroteskRegular.ttf"
          ]
        }
      ]
    ]
  }
}